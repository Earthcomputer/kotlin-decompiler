open class TestCastPrimitiveToObject {
   private var i: int = 0
   private var o: Object = null

   open fun test() {
      o = (long)i;// 8
      System.out.println(o.getClass());// 9
      System.out.println(o);// 10
      o = i;// 12
      System.out.println(o.getClass());// 13
      System.out.println(o);// 14
      o = 0;// 16
      System.out.println(o.getClass());// 17
      System.out.println(o);// 18
   }// 19
}

