               break;
            }

            if (decSeen) {// 78
               throw new NumberFormatException("multiple points");// 80
            }

            decPt = i;// 82
            if (signSeen) {// 83
               decPt = i - 1;// 84
            }

            decSeen = true;// 86
         }
      }

      return decPt;// 92
   }
}

